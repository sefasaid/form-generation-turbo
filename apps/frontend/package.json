{
  "name": "@repo/frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "test -f ../../apps/frontend/src/app/_types/generated/index.ts && echo 'Prisma types already generated, skipping...' || (test -f ../../turbo.json && turbo run generate --filter=@repo/prisma || (echo 'Generating prisma types directly...' && cd ../../packages/prisma && yarn generate))",
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint . --max-warnings 0",
    "start": "next start",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "material-symbols": "^0.40.2",
    "next": "~16.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io-client": "^4.8.3",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/nextFetch": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "5.9.2"
  }
}