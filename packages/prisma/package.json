{
  "name": "@repo/prisma",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.lib.json && cp -r src/generated dist/generated",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "tsx seed.ts",
    "studio": "prisma studio",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "@prisma/adapter-pg": "^7.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "dotenv": "^16.4.5",
    "prisma": "^7.3.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.2"
  },
  "prisma": {
    "seed": "tsx seed.ts"
  }
}