{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "^generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "env": [
        "NEXT_PUBLIC_API_URL",
        "DATABASE_URL"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "env": [
        "NEXT_PUBLIC_API_URL"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "e2e": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "e2e:open": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "migrate": {
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "cache": false
    }
  }
}